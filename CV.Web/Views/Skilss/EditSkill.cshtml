@model CV.Web.Models.Entity.TblSkills
@{
    ViewBag.Title = "EditSkill";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4> YETENEKLERİMİ DÜZENLE</h4>
<br />
<form method="post">
    <br />
    @Html.HiddenFor(x => x.ID)
    <br />
    @Html.Label("Yeteneklerim")
    @Html.TextBoxFor(x => x.Skill, new { @class = "form-control" })
    <br />
    <button class="btn btn-info"><i class="fa fa-pencil-alt"></i>GÜNCELLE</button>
</form>

