@using CV.Web.Models.Entity
@model List<TblSkills>
<div class="resume-section-content container">
    <ul class="list-inline dev-icons portfolio-filter list-inline wow fadeInUp">
        <div class="resume-section-content">
            <h2 class="mb-5">YETENEKLERİM</h2>
            <h3> <div class="subheading mb-3">Programlama Dili & Araçlar</div></h3>
            <ul class="list-inline dev-icons">
                <li class="list-inline-item" style="width:30px;"><i class="fab fa-html5"></i></li>
                <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
                <li class="list-inline-item"><i class="fab fa-js-square"></i></li>
                <li class="list-inline-item"><i class="fab fa-wordpress"></i></li>
                <li class="list-inline-item"><i class="fab fa-python"></i></li>
                <li class="list-inline-item"><i class="fab fa-"></i></li>
            </ul>
            <br />

            <h3><div class="subheading mb-3">Becerilerim</div></h3>
            <ul class="fa-ul mb-0">
                @foreach (var x in Model)
                {
                    <li>
                        <span class="fa-li"><i class="fas fa-check"></i></span>
                        @x.Skill
                    </li>
                }
            </ul>
        </div>
    </ul>
</div>